                            <Rectangle x:Name="BackgroundGradient" RadiusX="1" RadiusY="1" StrokeThickness="1" Margin="1" Stroke="#FFFFFFFF">
                                <Rectangle.Fill>
                                    <LinearGradientBrush StartPoint="0,.7" EndPoint="1,.7">
