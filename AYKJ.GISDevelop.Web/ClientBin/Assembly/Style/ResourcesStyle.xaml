                                <ScrollViewer x:Name="ScrollViewer" BorderThickness="0" Padding="1" Background="#0C000000" Style="{StaticResource ScrollViewerStyle}" MaxHeight="112" Margin="0">
                                    <ItemsPresenter Margin="0"/>
