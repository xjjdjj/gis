        	<StackPanel Margin="0,10">
        		<StackPanel Orientation="Horizontal" Margin="10,0,0,0">
        			<RoundSlider:RoundSlider x:Name="sd" ValueChanged="sd_ValueChanged" VerticalAlignment="Center" Margin="0" SmallChange="1" LargeChange="1" HorizontalAlignment="Left" Width="150" Style="{StaticResource SliderStyle}" Cursor="Hand"/>
        			<Button x:Name="btn_play" Click="btn_play_Click"  Content="播放" Style="{StaticResource ButtonStyle}" Height="17" VerticalAlignment="Center" Width="45" Background="#FF1F3B53" Foreground="#FF7EC307" Margin="10,0,0,0" Cursor="Hand"/>
        		</StackPanel>
        		<Border BorderBrush="#FF1D1E1E" BorderThickness="0,0,0,1" VerticalAlignment="Bottom" Margin="0,5,0,0"/>
        		<Grid Margin="0">
        			<StackPanel x:Name="Sp_FireExplosion" HorizontalAlignment="Left" Orientation="Horizontal" Margin="10,5,0,0" Height="19" VerticalAlignment="Top">
        				<RadioButton x:Name="rbtn_sw" Checked="rbtnFireExplosion_Checked" Content="死亡区域" Style="{StaticResource RadioButtonStyle1}" Width="73" HorizontalAlignment="Left" Margin="0" IsChecked="True" Foreground="#FF7EC307" Cursor="Hand"/>
        				<RadioButton x:Name="rbtn_zs" Checked="rbtnFireExplosion_Checked" Content="重伤区域" Style="{StaticResource RadioButtonStyle1}" Width="73" HorizontalAlignment="Left" Margin="1,0,0,0" Foreground="#FF7EC307" Cursor="Hand"/>
        				<RadioButton x:Name="rbtn_qs" Checked="rbtnFireExplosion_Checked" Content="轻伤区域" Style="{StaticResource RadioButtonStyle1}" Width="73" HorizontalAlignment="Left" Margin="1,0,0,0" Foreground="#FF7EC307" Cursor="Hand"/>
        				<RadioButton x:Name="rbtn_aq" Checked="rbtnFireExplosion_Checked" Content="安全区域" Style="{StaticResource RadioButtonStyle1}" Width="73" HorizontalAlignment="Left" Margin="1,0,0,0" Foreground="#FF7EC307" Cursor="Hand"/>
        			</StackPanel>
        			<StackPanel x:Name="Sp_Leak" HorizontalAlignment="Left" Orientation="Horizontal" Margin="10,5,0,0" Height="19" VerticalAlignment="Top" Visibility="Collapsed">
        				<RadioButton x:Name="rbtn_zd" Checked="rbtnLeak_Checked" Content="中毒区域" Style="{StaticResource RadioButtonStyle1}" Width="73" HorizontalAlignment="Left" Margin="2,0,0,0" IsChecked="True" Cursor="Hand"/>
        				<RadioButton x:Name="rbtn_rs" Checked="rbtnLeak_Checked" Content="燃烧区域" Style="{StaticResource RadioButtonStyle1}" Width="73" HorizontalAlignment="Left" Margin="1,0,0,0" Cursor="Hand"/>
        			</StackPanel>
        			<StackPanel x:Name="Sp_Leak2" HorizontalAlignment="Left" Orientation="Horizontal" Margin="-4,5,0,0" Height="19" VerticalAlignment="Top" Visibility="Collapsed">
        				<RadioButton x:Name="rbtn_ss" Checked="rbtnLeak_Checked" Content="泄漏区域" Style="{StaticResource RadioButtonStyle1}" Width="73" HorizontalAlignment="Left" Margin="2,0,0,0" IsChecked="True" Cursor="Hand"/>
        			</StackPanel>
        			<sdk:DataGrid x:Name="data_result" Margin="10,30,0,0" LoadingRow="data_result_LoadingRow" IsReadOnly="True" AutoGenerateColumns="False" FontSize="12" FontFamily="Nsimsun" 
        				MinWidth="410" Template="{StaticResource DataGridControlTemplate}" CellStyle="{StaticResource DataGridCellStyle}" 
        				ColumnHeaderStyle="{StaticResource DataGridColumnHeaderStyle}" RowStyle="{StaticResource DataGridRowStyle}" 
        				Background="{x:Null}" BorderBrush="{x:Null}" BorderThickness="0" Foreground="White" RowBackground="#00EAEFF4" VerticalAlignment="Top" Height="120" HorizontalAlignment="Left">
        				<sdk:DataGrid.Columns>
        					<sdk:DataGridTemplateColumn Header="选择" MinWidth="45" MaxWidth="45">
        						<sdk:DataGridTemplateColumn.CellTemplate>
        							<DataTemplate>
        								<StackPanel Orientation="Horizontal" VerticalAlignment="Center" HorizontalAlignment="Center">
        									<CheckBox x:Name="ck" Checked="ck_Checked" Unchecked="ck_Unchecked" Tag="{Binding ID}" Style="{StaticResource CheckBoxStyle}" BorderThickness="0" Foreground="White"/>
        								</StackPanel>
        							</DataTemplate>
        						</sdk:DataGridTemplateColumn.CellTemplate>
        					</sdk:DataGridTemplateColumn>
        					<sdk:DataGridTextColumn  Header="类别"  Binding="{Binding wxytype}" MinWidth="76" />
        					<sdk:DataGridTextColumn  Header="名称"  Binding="{Binding wxyname}" MinWidth="253" MaxWidth="253" />
        					<sdk:DataGridTemplateColumn Header="定位" MaxWidth="35" MinWidth="35">
        						<sdk:DataGridTemplateColumn.CellTemplate>
        							<DataTemplate>
        								<StackPanel Orientation="Horizontal" VerticalAlignment="Center" HorizontalAlignment="Center">
        									<Button x:Name="btn_postion" Cursor="Hand" Height="16" Style="{StaticResource ButtonStyle_Postion}" Width="16" Click="btn_postion_Click"/>
        								</StackPanel>
        							</DataTemplate>
        						</sdk:DataGridTemplateColumn.CellTemplate>
        					</sdk:DataGridTemplateColumn>
        				</sdk:DataGrid.Columns>
        			</sdk:DataGrid>
        		</Grid>
        		<Button x:Name="btn_route" Click="btn_route_Click" Content="疏散路径" HorizontalAlignment="Left" Style="{StaticResource ButtonStyle}" Background="#FF1F3B53" Foreground="#FF7EC307" Width="73" Height="19" VerticalAlignment="Bottom" Margin="10,5,0,0" Cursor="Hand"/>
        	</StackPanel> 	
        </Border>

    </Grid>
</UserControl>
