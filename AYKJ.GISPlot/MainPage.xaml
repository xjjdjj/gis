        					<ComboBox x:Name="cbx_TxtFontSize" Margin="133,0,0,0" VerticalAlignment="Center" Grid.Column="1" Grid.Row="2" HorizontalAlignment="Left" Width="60" Height="20" BorderThickness="0" Style="{StaticResource ComboBoxStyle1}" ItemContainerStyle="{StaticResource ComboBoxItemStyle1}" Foreground="White" Cursor="Hand"/>
