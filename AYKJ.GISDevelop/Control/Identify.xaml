            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="Button">
                        <Grid>
                            <Path Data="M 584.188,953.595L 598.352,953.595L 598.352,929.623C 594.518,938.165 589.754,946.199 584.188,953.595 Z " Fill="#FF858E93" RenderTransformOrigin="0.5,0.5" Stretch="Fill" StrokeThickness="1" StrokeLineJoin="Round" UseLayoutRounding="False" Margin="0,0,2,0" Width="8" Height="8">
