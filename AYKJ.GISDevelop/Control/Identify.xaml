            </Grid>
            <Grid Margin="0" Grid.Row="1">
                <Border>
                    <Border.Resources>
                        <sdk:HierarchicalDataTemplate x:Key="childTemplate" ItemsSource="{Binding Children}">
                            <StackPanel Orientation="Horizontal">
                                <Button Content="{Binding Title}" />
                            </StackPanel>
                        </sdk:HierarchicalDataTemplate>
                        <sdk:HierarchicalDataTemplate x:Key="treeTemplate" ItemsSource="{Binding Children}"  >
                            <TextBlock Text="{Binding Title}" />
                        </sdk:HierarchicalDataTemplate>
                    </Border.Resources>
                    <sdk:TreeView x:Name="layertree"
                                ItemTemplate="{StaticResource treeTemplate}" 
                                ItemsSource="{Binding}"           
                				SelectedItemChanged="treeView_SelectedItemChanged"
                				Grid.Column="0"
                				Margin="10,0,15,0" 
                				BorderThickness="0" 
                			 Background="#19FFFFFF" BorderBrush="{x:Null}"  Foreground="White"  FontSize="12" FontFamily="NSimSun" Style="{StaticResource TreeViewStyle}" ItemContainerStyle="{StaticResource TreeViewItemStyle}"/>
                </Border>
            </Grid>
            <Border BorderBrush="Black" BorderThickness="0,1,0,0" Margin="0" Grid.Row="2">
                <Grid Margin="0" Height="30" VerticalAlignment="Bottom">
                    <TextBlock HorizontalAlignment="Left" Margin="10,0,0,0" TextWrapping="Wrap" Text="位置：" VerticalAlignment="Center" Foreground="White" FontFamily="NSimSun" FontSize="12"/>
                    <TextBox x:Name="txt_coor" Margin="57,0,13,0" TextWrapping="Wrap" FontFamily="NSimSun" FontSize="12" VerticalAlignment="Center" IsReadOnly="True" Style="{StaticResource TextBoxStyle}">
                        <TextBox.Background>
                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                <GradientStop Color="#FFE8E8E8" Offset="0"/>
                                <GradientStop Color="#FFCCCBCC" Offset="1"/>
                            </LinearGradientBrush>
                        </TextBox.Background>
                    </TextBox>
                </Grid>
            </Border>
            <Grid Margin="0" Grid.Row="3">
                <sdk:DataGrid x:Name="dataresult" Margin="10,4,10,4" AutoGenerateColumns="true" 
