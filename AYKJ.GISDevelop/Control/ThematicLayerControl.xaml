        					</StackPanel>
        				</Grid>
                        <Border BorderBrush="Black" BorderThickness="1,1,1,0" Margin="10,5,0,0"/>
                        <Border BorderBrush="Black" BorderThickness="1,1,1,0" Margin="10,1,0,0"/>
        				<Grid x:Name="toc_grid" Margin="10,2,10,10" HorizontalAlignment="Left"/>
        			</StackPanel>
                </ScrollViewer>
            </Border>
        </StackPanel>
    </Grid>
</UserControl>
