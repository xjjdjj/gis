                            </VisualStateManager.VisualStateGroups>
                            <Path Data="F1 M 154.772,251.654L 180.846,238.022L 180.86,247.568L 163.963,255.98L 180.883,264.346L 180.896,273.892L 154.784,260.333L 154.772,251.654 Z " Height="11" Stretch="Uniform" Width="11">
